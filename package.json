{
    "name": "master-book",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "install": "cd server && npm install cd ../client && npm install",
        "start": "concurrently \"cd server && nodemon server.js\" \"cd client && npm run serve\"",
        "serve": "cd server && nodemon server.js",
        "cli": "cd client && npm run serve"
    },
    "devDependencies": {
        "concurrently": "^8.2.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {},
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "dependencies": {
        "jsonwebtoken": "^9.0.0",
        "nodemon": "^2.0.22"
    }
}
